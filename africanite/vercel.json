{
    "version": 2,
    "routes": [
        {
            "src": "/static/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            },
            "dest": "/static/$1"
        },
        {
            "src": "/favicon.png",
            "headers": {
                "Cache-Control": "public, max-age=86400"
            },
            "dest": "/favicon.png"
        },
        {
            "src": "/manifest.json",
            "headers": {
                "Cache-Control": "public, max-age=86400"
            },
            "dest": "/manifest.json"
        },
        {
            "src": "/(.*)",
            "headers": {
                "Cache-Control": "no-cache"
            },
            "dest": "/index.html"
        }
    ]
}